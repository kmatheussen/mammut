

FAST INSTALL
        cd juce_1_44/juce/build/linux
        CONFIG=Release CFLAGS='-Wno-narrowing' make -j8
	cd ../../../../src
	make -f Makefile.linux -j8
	(sudo) make -f Makefile.linux install



REQUIRED LIBRARIES
	libjack
	libsamplerate
	libsndfile
	libvorbisfile


